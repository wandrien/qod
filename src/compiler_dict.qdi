	when !T_IsFunc(T):
		StopInternal(__FILE__, __LINE__);
	when Dict[T].pArgs != nDICT:
		StopInternal(__FILE__, __LINE__);

	word pARGS = DictAlloc();
	Dict[pARGS].Class = CLASS.cARGS;
