/*****************************************************************************/

DICT_HASHMAP DictHashMap[nDICT / 10];
word nDictHashMap = 1;

word AllocateDictHashMap() of
	when nDictHashMap >= countof(DictHashMap):
		return 0;
	word iHashMap = nDictHashMap;
	inc nDictHashMap;

	word I = 0;
	DICT_HASHMAP @HashMap = @DictHashMap[iHashMap];
	while I < countof(HashMap.Buckets) loop
		HashMap.Buckets[I] = nDICT;
		inc I;
	end:while

	return iHashMap;
end

