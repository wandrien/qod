word T_Array(word T; word S)
	word P = tArrayTypes;
	while P < nDict do
		DICT @d = @Dict[P];
		when d.Class != CLASS.cTYPE | d.Sub != SUBCLASS.sARRAY:
			StopInternal(__FILE__, __LINE__);
		if d.pType == T
		 & d.TypeSize == S then
			return P;
		end:if
		P = d.tLink;
	end:while

	P = DictAlloc();
	DICT @d = @Dict[P];
	d.Class    = CLASS.cTYPE;
	d.Sub      = SUBCLASS.sARRAY;
	d.pType    = T;
	d.TypeSize = S;
	d.tLink    = tArrayTypes;
	tArrayTypes = P;
	return P;
end

