// TEST: should_print '25 35 '

include "../sys4lnx.ctxi"
include "../include/hosted.ctxi"

namespace XXX of
	synonym u16 uint16
	synonym u32 uint32
end

XXX.u32 foo1(XXX.u32 x; XXX.u16 y)
	return x + y;
end

uint32 foo2(uint32 x; uint16 y)
	return x + y;
end

begin
	uint x1 = 20;
	XXX.u32 v25 = foo1(XXX.u32(x1), XXX.u16(5));
	XXX.u32 v35 = foo2(XXX.u32(x1), XXX.u16(15));

	puts(@str(v25)); puts(" ");
	puts(@str(v35)); puts(" ");
end
