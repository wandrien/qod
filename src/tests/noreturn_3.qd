// TEST: compilation_should_fail 'Управление достигает конца noreturn функции'

include "../include/test_env.qdi";

word s = 0;

noreturn nr1(word a) of
	switch s of
	case 1:
		if a < 10 then
			put_word(a + 1); puts(" ");
			terminate(0);
		else
			put_word(a); puts(" ");
		end:if
	default:
		terminate(0);
	end:switch
end

void f1(word a) of
	nr1(a);
	nr1(a + 1);
end

termination_status main() of
	s = 1;
	f1(1000);
	return 0;
end
