// TEST: should_print '1000 '

include "../include/test_env.qdi";

word s = 0;

noreturn nr1(word a) of
	switch s of
	case 1:
		if a < 10 then
			put_word(a + 1); puts(" ");
			halt(0);
		else
			put_word(a); puts(" ");
			halt(0);
		end:if
	default:
		halt(0);
	end:switch
end

void f1(word a) of
	nr1(a);
	nr1(a + 1);
end

word main() of
	s = 1;
	f1(1000);
	return 0;
end
