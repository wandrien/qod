// TEST: should_print 'OK1 OK2 OK3 OK4 OK5 OK6 '

include "../sys4lnx.ctxi"
include "../include/hosted.ctxi"

int p1 = 1;
int p2 = 2;
int p3 = 3;
int p4 = 4;
int p5 = 5;

int m5 = -5;

void foo()

	if p5 - p3 - p2 == 0 then
		puts("OK1 ");
	else
		puts("OOPS ");
	end:if

	if p5 - p3 - p2 != 0 then
		puts("OOPS ");
	else
		puts("OK2 ");
	end:if

	if m5 + p5 == 0 then
		puts("OK3 ");
	else
		puts("OOPS ");
	end:if

	if p3 + m5 < 0 then
		puts("OK4 ");
	else
		puts("OOPS ");
	end:if

	if m5 + p5 >= 0 then
		puts("OK5 ");
	else
		puts("OOPS ");
	end:if

	if p1 - 1 >= 0 then
		puts("OK6 ");
	else
		puts("OOPS ");
	end:if
end

begin
	foo();
end
