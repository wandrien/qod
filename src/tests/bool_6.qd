// TEST: should_print 'OK1 OK2 OK3 OK4 '

include "../sys4lnx.qdi"
include "../include/hosted.qdi"

bool t()
	return true;
end

bool f()
	return false;
end

void foo()

	if t() == t() then
		puts("OK1 ");
	else
		puts("OOPS ");
	end:if

	if f() == f() then
		puts("OK2 ");
	else
		puts("OOPS ");
	end:if

	if f() != t() then
		puts("OK3 ");
	else
		puts("OOPS ");
	end:if

	if t() == f() then
		puts("OOPS ");
	else
		puts("OK4 ");
	end:if

end

begin
	foo();
end
