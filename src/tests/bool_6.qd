// TEST: should_print 'OK1 OK2 OK3 OK4 '

include "../include/test_env.qdi";

bool t() of
	return true;
end

bool f() of
	return false;
end

void foo() of

	if t() == t() then
		puts("OK1 ");
	else
		puts("OOPS ");
	end:if

	if f() == f() then
		puts("OK2 ");
	else
		puts("OOPS ");
	end:if

	if f() != t() then
		puts("OK3 ");
	else
		puts("OOPS ");
	end:if

	if t() == f() then
		puts("OOPS ");
	else
		puts("OK4 ");
	end:if

end

termination_status main() of
	foo();
	return 0;
end
