// TEST: should_print 'OK1 OK2 '

include "../sys4lnx.ctxi"
include "../include/hosted.ctxi"

word f1(word x; word y)
	word a = (%(x) + (*1*)%);
	word b = ((*y * 2*));
	return a + b;
end

void foo()
	word t1 = sizeof(%int%);
	word t2 = sizeof(*char*);

	word v5 = const_expr(%(4) + 1%);

	if f1(*2, 4*) == f1(%2, 4%) then
		puts("OK1 ");
	else
		puts("OOPS ");
	end:if

	if f1(v5, 10) == (%(*26*)%) then
		puts("OK2 ");
	else
		puts("OOPS ");
	end:if

end

begin
	foo();
end
