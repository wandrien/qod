// TEST: should_print 'OK1 OK2 '

include "../include/test_env.qdi";

word f1(word x; word y) of
	word a = (%(x) + (*1*)%);
	word b = ((*y * 2*));
	return a + b;
end

void foo() of
	word t1 = sizeof(%int%);
	word t2 = sizeof(*char*);

	word v5 = const_expr(%(4) + 1%);

	if f1(*2, 4*) == f1(%2, 4%) then
		puts("OK1 ");
	else
		puts("OOPS ");
	end:if

	if f1(v5, 10) == (%(*26*)%) then
		puts("OK2 ");
	else
		puts("OOPS ");
	end:if

end

termination_status main() of
	foo();
	return 0;
end
