// TEST: compilation_should_fail 'Оператор return в теле noreturn функции'

include "../include/test_env.qdi";

noreturn nr1(word a) of
	when a == 0:
		return;
	put_word(a); puts(" ");
	terminate(1);
end

void f1(word a) of
	nr1(a);
	nr1(a + 1);
end

termination_status main() of
	f1(1000);
	return 0;
end
