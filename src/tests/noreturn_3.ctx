// TEST: compilation_should_fail 'Управление достигает конца noreturn функции'

include "../sys4lnx.ctxi"
include "../include/hosted.ctxi"

word s = 0;

noreturn nr1(word a)
	switch s of
	case 1:
		if a < 10 then
			put_word(a + 1); puts(" ");
			halt(0);
		else
			put_word(a); puts(" ");
		end:if
	default:
		halt(0);
	end:switch
end

void f1(word a)
	nr1(a);
	nr1(a + 1);
end

begin
	s = 1;
	f1(1000);
end
