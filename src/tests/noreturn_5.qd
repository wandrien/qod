// TEST: should_print '1000 '

include "../include/test_env.qdi";

word s = 0;

noreturn nr1(word a) of
	while a > 100 do
		if s == 1 then
			put_word(a); puts(" ");
			terminate(0);
		else
			terminate(0);
			exit;
		end:if
	end:do
end

void f1(word a) of
	nr1(a);
	nr1(a + 1);
end

termination_status main() of
	s = 1;
	f1(1000);
	return 0;
end
