
word ResolveLocal(char @Name)
	word  I=0;
	while I<nLocal do
		when strcmp(@Local[I].Name,@Name)=0:
			return I;
		inc I;
	end:while
	return nLOCAL;
end


word ResolveParm(char @Name)
	word I=Dict[pFunc].pIndx;
	while I<Dict[pFunc].nIndx do
		when strcmp(@Parm[I].Name,@Name)=0:
			return I;
		inc I;
	end:while
	return nPARM;
end

